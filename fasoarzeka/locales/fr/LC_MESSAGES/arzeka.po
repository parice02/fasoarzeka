#: fasoarzeka/arzeka.py:150
msgid "Authentication token is not set. Please authenticate first."
msgstr "Le jeton d'authentification n'est pas défini. Veuillez vous authentifier d'abord."

#: fasoarzeka/arzeka.py:188
msgid "Request timeout after %(timeout)s seconds"
msgstr "Délai d'attente écoulé après %(timeout)s secondes"

#: fasoarzeka/arzeka.py:194
msgid "Failed to connect to Arzeka API: %(err)s"
msgstr "Échec de la connexion à l'API Arzeka : %(err)s"

#: fasoarzeka/arzeka.py:205
msgid "API request failed: %(err)s"
msgstr "La requête API a échoué : %(err)s"

#: fasoarzeka/arzeka.py:212
msgid "Unexpected error: %(err)s"
msgstr "Erreur inattendue : %(err)s"

#: fasoarzeka/arzeka.py:423
msgid "Automatic re-authentication failed: %(err)s"
msgstr "La ré-authentification automatique a échoué : %(err)s"

#: fasoarzeka/arzeka.py:453
msgid "username must be a non-empty string"
msgstr "Le nom d'utilisateur doit être une chaîne non vide"

#: fasoarzeka/arzeka.py:456
msgid "password must be a non-empty string"
msgstr "Le mot de passe doit être une chaîne non vide"

#: fasoarzeka/arzeka.py:494
msgid "Invalid response format from authentication endpoint"
msgstr "Format de réponse invalide depuis le point d'authentification"

#: fasoarzeka/arzeka.py:501
msgid "Authentication response missing required fields"
msgstr "La réponse d'authentification ne contient pas les champs requis"

#: fasoarzeka/arzeka.py:527
msgid "Invalid credentials: username or password is incorrect"
msgstr "Identifiants invalides : le nom d'utilisateur ou le mot de passe est incorrect"

#: fasoarzeka/arzeka.py:531
msgid "Access forbidden: account may be locked or inactive"
msgstr "Accès interdit : le compte peut être bloqué ou inactif"

#: fasoarzeka/arzeka.py:540
msgid "Authentication request failed: %(err)s"
msgstr "La requête d'authentification a échoué : %(err)s"

#: fasoarzeka/arzeka.py:548
msgid "Failed to connect to authentication endpoint: %(err)s"
msgstr "Échec de la connexion au point d'authentification : %(err)s"

#: fasoarzeka/arzeka.py:554
msgid "Authentication request timeout after %(timeout)s seconds"
msgstr "Délai d'attente de la requête d'authentification après %(timeout)s secondes"

#: fasoarzeka/arzeka.py:565
msgid "Authentication failed with unexpected error: %(err)s"
msgstr "L'authentification a échoué avec une erreur inattendue : %(err)s"

#: fasoarzeka/arzeka.py:604
msgid "amount must be a positive number greater than %(min)s"
msgstr "Le montant doit être un nombre positif supérieur à %(min)s"

#: fasoarzeka/arzeka.py:609
msgid "merchant_id must be a non-empty string/int"
msgstr "merchant_id doit être une chaîne ou un entier non vide"

#: fasoarzeka/arzeka.py:615
msgid "additional_info must contain firstname, lastname, and mobile"
msgstr "additional_info doit contenir firstname, lastname et mobile"

#: fasoarzeka/arzeka.py:641
msgid "When generateReceipt is True, additional_info must contain: %(fields)s"
msgstr "Lorsque generateReceipt est True, additional_info doit contenir : %(fields)s"

#: fasoarzeka/arzeka.py:622
msgid "additional_info fields firstname, lastname, and mobile cannot be empty or null"
msgstr "Les champs additional_info firstname, lastname et mobile ne peuvent pas être vides ou nuls"

#: fasoarzeka/arzeka.py:704
msgid "mapped_order_id must be a non-empty string"
msgstr "mapped_order_id doit être une chaîne non vide"

#: fasoarzeka/arzeka.py:883
msgid "Not authenticated. Please call authenticate() first."
msgstr "Non authentifié. Veuillez appeler authenticate() d'abord."
